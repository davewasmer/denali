{
  "name": "denali",
  "version": "0.0.3",
  "description": "An opinionated Node framework for building robust JSON APIs",
  "homepage": "https://github.com/davewasmer/denali",
  "repository": "davewasmer/denali",
  "author": {
    "name": "Dave Wasmer",
    "email": "davewasmer@gmail.com",
    "url": "davewasmer.com"
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "denali": "bin/denali"
  },
  "main": "dist/index.js",
  "keywords": [
    "jsonapi",
    "framework",
    "resources",
    "opinionated",
    "denali"
  ],
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "coveralls": "^2.11.4",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.2",
    "in-publish": "2.0.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "must": "^0.13.1",
    "rimraf": "^2.4.2",
    "watch": "^0.16.0"
  },
  "scripts": {
    "prepublish": "in-publish && npm run lint && npm test && npm run coverage && npm run compile || echo ''",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register",
    "lint": "eslint lib test",
    "build": "npm run clean && babel lib --out-dir dist",
    "coverage": "babel-node isparta cover _mocha && coveralls < coverage/lcov.info",
    "postcoverage": "rimraf coverage",
    "watch": "watch 'npm run build && npm run test' lib test",
    "docs": "docify build",
    "clean": "rimraf dist/*"
  },
  "license": "MIT",
  "dependencies": {
    "babel": "^6.1.18",
    "blackburn": "0.0.2",
    "bluebird": "^2.10.0",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "core-object": "^1.1.0",
    "dag-map": "^1.0.2",
    "diveSync": "^0.3.0",
    "express": "^4.13.3",
    "findup-sync": "^0.3.0",
    "foraker": "0.0.4",
    "glob": "^6.0.1",
    "http-errors": "^1.3.1",
    "inflection": "^1.8.0",
    "liftoff": "^2.2.0",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "require-all": "^2.0.0",
    "require-dir": "^0.3.0",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^2.0.2",
    "trash": "^3.4.1"
  }
}
